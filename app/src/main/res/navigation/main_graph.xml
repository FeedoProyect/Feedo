<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/menuFragment">

        <fragment
            android:id="@+id/menuFragment"
            android:name="com.benjamin.proyectofeedo.UI.feedoMenu.MenuFragment"
            android:label="fragment_menu"
            tools:layout="@layout/fragment_menu">
            <action
                android:id="@+id/action_menuFragment_to_catalogosListFragment"
                app:destination="@id/catalogosListFragment" />
            <action
                android:id="@+id/action_menuFragment_to_buscadorPrincipalFragment"
                app:destination="@id/buscadorPrincipalFragment" />
        </fragment>
        <fragment
            android:id="@+id/catalogosListFragment"
            android:name="com.benjamin.proyectofeedo.UI.feedoContenidoCatalogos.CatalogosListComidasFragment"
            android:label="fragment_catalogos_list"
            tools:layout="@layout/fragment_catalogos_list">
            <argument
                android:name="type"
                app:argType="com.benjamin.proyectofeedo.domain.model.CatalogosModel" />
        </fragment>
        <fragment
            android:id="@+id/buscadorPrincipalFragment"
            android:name="com.benjamin.proyectofeedo.UI.feedoBuscador.BuscadorPrincipalFragment"
            android:label="fragment_buscador_principal"
            tools:layout="@layout/fragment_buscador_principal" />
        <fragment
            android:id="@+id/perfilFragment"
            android:name="com.benjamin.proyectofeedo.UI.feedoPerfil.PerfilFragment"
            android:label="fragment_historial"
            tools:layout="@layout/fragment_historial" />
        <fragment
            android:id="@+id/addRecetasFragment"
            android:name="com.benjamin.proyectofeedo.UI.feedoAddRecetas.AddRecetaFragment"
            android:label="fragment_favoritos"
            tools:layout="@layout/fragment_favoritos" />
</navigation>